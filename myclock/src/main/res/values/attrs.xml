<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--自定义属性集合:CircleView-->
    <!--在该属性下，设置不同的自定义属性-->
    <declare-styleable name="CircleView">
        <!--在attr标签下设置需要的属性值-->
        <!--此处定义了一个设置图形颜色:circle_color属性,格式是color,代表颜色或者reference代表引用资源-->
        <!--格式有很多种，如资源id(reference)等等-->
        <attr name="circle_color" format="reference|color"/>
    </declare-styleable>
</resources>
